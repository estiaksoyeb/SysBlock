<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- We use a standard DeviceDefault theme. 
         This is built-in to every Android phone, so it never causes "Not Found" errors. -->
    <style name="Theme.SysBlock" parent="@android:style/Theme.DeviceDefault.NoActionBar">
        <!-- Status bar color -->
        <item name="android:statusBarColor">#000000</item>
        <!-- Background color for the window behind your Compose content -->
        <item name="android:windowBackground">#000000</item>
    </style>
</resources>